{
  "name": "inkblot-serf",
  "version": "2.0.0",
  "author": "Nate Riffe",
  "license": "Apache-2.0",
  "summary": "A Puppet module to deploy and manage the Serf agent.",
  "source": "https://github.com/inkblot/puppet-serf",
  "project_page": "https://github.com/inkblot/puppet-serf",
  "issues_url": "https://github.com/inkblot/puppet-serf/issues",
  "tags": [ "serf" ],
  "operatingsystem_support": [
    {
